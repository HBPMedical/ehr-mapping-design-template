      <confidence>1.0</confidence>
    </correspondence>
    <correspondence>
      <source-paths />
      <source-value>"CLM_JACK"</source-value>
      <target-path>i2b2_capture.observation_fact.observation_factTuple.provider_id</target-path>
      <transformation-function>"CLM_JACK"</transformation-function>
      <confidence>1.0</confidence>
    </correspondence>
    <correspondence>
      <source-paths />
      <source-value>datetime()</source-value>
      <target-path>i2b2_capture.observation_fact.observation_factTuple.import_date</target-path>
      <transformation-function>datetime()</transformation-function>
      <confidence>1.0</confidence>
    </correspondence>
  </correspondences>
</mappingtask>

