<?xml version="1.0" encoding="UTF-8"?>
<mappingtask>
  <config>
    <rewriteSubsumptions>true</rewriteSubsumptions>
    <rewriteCoverages>true</rewriteCoverages>
    <rewriteSelfJoins>true</rewriteSelfJoins>
    <rewriteEGDs>false</rewriteEGDs>
    <sortStrategy>-1</sortStrategy>
    <skolemTableStrategy>-1</skolemTableStrategy>
    <useLocalSkolem>false</useLocalSkolem>
  </config>
  <source>
    <type>CSV</type>
    <csv>
      <csv-db-name>CLM_JACK</csv-db-name>
      <csv-tables>
        <csv-table>
          <schema>source/clm_metadata.csv</schema>
          <instances>
            <instance>
              <path>source/clm_metadata.csv</path>
              <column-names>true</column-names>
            </instance>
          </instances>
        </csv-table>
        <csv-table>
          <schema>source/unpivoted_volumes_new.csv</schema>
          <instances>
            <instance>
              <path>source/unpivoted_volumes_new.csv</path>
              <column-names>true</column-names>
            </instance>
          </instances>
        </csv-table>
        <csv-table>
          <schema>source/DB_demo.csv</schema>
          <instances>
            <instance>
              <path>source/DB_demo.csv</path>
              <column-names>true</column-names>
            </instance>
          </instances>
        </csv-table>
        <csv-table>
          <schema>source/unpivoted_DB_EHR_subset_cp2.csv</schema>
          <instances>
            <instance>
              <path>source/unpivoted_DB_EHR_subset_cp2.csv</path>
              <column-names>true</column-names>
            </instance>
          </instances>
        </csv-table>
        <csv-table>
          <schema>source/cdes_clm.csv</schema>
          <instances>
            <instance>
              <path>source/cdes_clm.csv</path>
              <column-names>true</column-names>
            </instance>
          </instances>
        </csv-table>
      </csv-tables>
    </csv>
    <inclusions />
    <exclusions />
    <duplications />
    <functionalDependencies />
    <selectionConditions />
    <joinConditions />
  </source>
  <target>
    <type>Relational</type>
    <relational>
      <driver>org.postgresql.Driver</driver>
      <uri>jdbc:postgresql://localhost:45432/i2b2_capture</uri>
      <login>postgres</login>
      <password>1234</password>
    </relational>
    <inclusions />
    <exclusions />
    <duplications />
    <functionalDependencies />
    <selectionConditions />
    <joinConditions />
  </target>
  <correspondences>
    <correspondence>
      <source-paths />
      <source-value>"CLM_JACK"</source-value>
      <target-path>i2b2_capture.provider_dimension.provider_dimensionTuple.provider_id</target-path>
      <transformation-function>"CLM_JACK"</transformation-function>
      <confidence>1.0</confidence>
    </correspondence>
    <correspondence>
      <source-paths />
      <source-value>"CLM_JACK"</source-value>
      <target-path>i2b2_capture.provider_dimension.provider_dimensionTuple.provider_path</target-path>
      <transformation-function>"CLM_JACK"</transformation-function>
      <confidence>1.0</confidence>
    </correspondence>
    <correspondence>
      <source-paths>
        <source-path>CLM_JACK.DB_demo.DB_demoTuple._SUBJECT_CODE</source-path>
      </source-paths>
      <target-path>i2b2_capture.patient_dimension.patient_dimensionTuple.patient_ide</target-path>
      <transformation-function>CLM_JACK.DB_demo.DB_demoTuple._SUBJECT_CODE</transformation-function>
      <confidence>1.0</confidence>
    </correspondence>
    <correspondence>
      <source-paths>
        <source-path>CLM_JACK.DB_demo.DB_demoTuple.GENDER</source-path>
      </source-paths>
      <target-path>i2b2_capture.patient_dimension.patient_dimensionTuple.sex_cd</target-path>
      <transformation-function>if((CLM_JACK.DB_demo.DB_demoTuple.GENDER == "Homme"), "M", if((CLM_JACK.DB_demo.DB_demoTuple.GENDER == "Femme"), "F", ""))</transformation-function>
      <confidence>1.0</confidence>
    </correspondence>
    <correspondence>
      <source-paths>
        <source-path>CLM_JACK.unpivoted_DB_EHR_subset_cp2.unpivoted_DB_EHR_subset_cp2Tuple.SUBJECT_CODE</source-path>
      </source-paths>
      <target-path>i2b2_capture.observation_fact.observation_factTuple.patient_ide</target-path>
      <transformation-function>CLM_JACK.unpivoted_DB_EHR_subset_cp2.unpivoted_DB_EHR_subset_cp2Tuple.SUBJECT_CODE</transformation-function>
      <confidence>1.0</confidence>
    </correspondence>
    <correspondence>
      <source-paths />
      <source-value>datetime()</source-value>
      <target-path>i2b2_capture.patient_dimension.patient_dimensionTuple.import_date</target-path>
      <transformation-function>datetime()</transformation-function>
